<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background Colors-->
    <SolidColorBrush x:Key="backgroundPrimaryColor" Color="#3D98A3"/>
    <Color x:Key="backgroundPrimaryColor1">#3D98A3</Color>
    <Color x:Key="backgroundPrimaryColor2">#6D92A3</Color>
    <Color x:Key="backgroundSecondaryColor1">#00FF33</Color>
    <Color x:Key="backgroundSecondaryColor2">#01CB33</Color>
    <Color x:Key="backgroundSecondaryColor3">#029120</Color>
